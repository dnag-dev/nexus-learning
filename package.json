{
  "name": "aauti-learn",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "db:migrate": "prisma migrate dev --schema=packages/db/prisma/schema.prisma",
    "db:seed:pg": "ts-node --project packages/db/tsconfig.json packages/db/prisma/seed.ts",
    "db:seed:neo4j": "ts-node --project packages/db/tsconfig.json packages/db/neo4j/knowledge-graph.seed.ts",
    "db:studio": "prisma studio --schema=packages/db/prisma/schema.prisma",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "turbo": "^2.3.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "npm@10.8.0",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0"
  }
}
