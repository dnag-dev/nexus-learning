{
  "name": "@aauti/db",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "echo 'no lint configured'",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "db:seed:pg": "ts-node prisma/seed.ts",
    "db:seed:neo4j": "ts-node neo4j/knowledge-graph.seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "neo4j-driver": "^5.25.0"
  },
  "devDependencies": {
    "prisma": "^5.20.0",
    "typescript": "^5.5.0",
    "ts-node": "^10.9.2",
    "@aauti/types": "*"
  }
}
